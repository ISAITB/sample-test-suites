    <soap:Header/>
    <soap:Body>
        <po:statusResponse correlationId="${correlationId}">
            <po:status>${status}</po:status>
            <po:processingDate>${processingDate}</po:processingDate>
        </po:statusResponse>
    </soap:Body>
</soap:Envelope>